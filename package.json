  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@jest/globals": "^29.7.0",
    "@vercel/ncc": "^0.38.3",
    "eslint-config-recommended": "^4.1.0",
    "eslint-plugin-jest": "^28.10.0",
    "globals": "^15.14.0",
    "jest": "^29.7.0"
  },
  "dependencies": {
